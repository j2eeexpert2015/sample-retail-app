{
  "id": null,
  "uid": "jmeter-load",
  "title": "JMeter Load Test Metrics",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m"],
    "time_options": ["5m", "10m", "15m", "30m"]
  },
  "panels": [
    {
      "type": "graph",
      "title": "Response Time Histogram",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_sum[5s]) / rate(http_response_time_histogram_count[5s])",
          "format": "time_series",
          "interval": "",
          "legendFormat": "Avg Response Time (ms)",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "graph",
      "title": "Threads Active",
      "targets": [
        {
          "expr": "jmeter_threads{state=\"active\"}",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "expr": "jmeter_threads{state=\"started\"}",
          "legendFormat": "started",
          "refId": "B"
        }
      ],
      "timeFrom": "30m",
      "hideTimeOverride": false,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ]
}
