{
  "id": null,
  "uid": "jmeter-corrected-metrics",
  "title": "JMeter Load Test Metrics (Corrected)",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-10m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["1s", "5s", "10s", "30s", "1m"],
    "time_options": ["5m", "10m", "15m", "30m", "1h"]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Active Threads",
      "targets": [
        {
          "expr": "http_active_threads_gauge",
          "legendFormat": "Active Threads",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#FF6B6B"
          },
          "unit": "short"
        }
      }
    },
    {
      "type": "stat",
      "title": "Current TPS",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_count[1m])",
          "legendFormat": "Transactions/sec",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "stat",
      "title": "Avg Response Time",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_sum[1m]) / rate(http_response_time_histogram_count[1m])",
          "legendFormat": "Avg Response Time (ms)",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1000},
              {"color": "red", "value": 3000}
            ]
          },
          "unit": "ms"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Response Time Trends",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_sum[1m]) / rate(http_response_time_histogram_count[1m])",
          "legendFormat": "Avg Response Time",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(http_response_time_histogram_bucket[1m]))",
          "legendFormat": "95th Percentile",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_response_time_histogram_bucket[1m]))",
          "legendFormat": "99th Percentile",
          "refId": "C"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ms"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Request Rate (TPS)",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_count[1m])",
          "legendFormat": "Requests/sec",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#4ECDC4"
          },
          "unit": "reqps"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Success vs Failure Rate",
      "targets": [
        {
          "expr": "rate(http_success_total[1m])",
          "legendFormat": "Success Rate",
          "refId": "A"
        },
        {
          "expr": "rate(http_failure_total[1m])",
          "legendFormat": "Failure Rate",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Success Rate"},
            "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "#56CA00"}}]
          },
          {
            "matcher": {"id": "byName", "options": "Failure Rate"},
            "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "#F2495C"}}]
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Response Time Distribution",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_bucket{le=\"100\"}[1m])",
          "legendFormat": "≤ 100ms",
          "refId": "A"
        },
        {
          "expr": "rate(http_response_time_histogram_bucket{le=\"500\"}[1m])",
          "legendFormat": "≤ 500ms",
          "refId": "B"
        },
        {
          "expr": "rate(http_response_time_histogram_bucket{le=\"1000\"}[1m])",
          "legendFormat": "≤ 1000ms",
          "refId": "C"
        },
        {
          "expr": "rate(http_response_time_histogram_bucket{le=\"3000\"}[1m])",
          "legendFormat": "≤ 3000ms",
          "refId": "D"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 24,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        }
      }
    }
  ]
}