{
  "id": null,
  "uid": "jmeter-http-metrics",
  "title": "JMeter HTTP Metrics Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 3,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m"],
    "time_options": ["5m", "10m", "15m", "30m"]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Active Threads",
      "targets": [
        {
          "expr": "jmeter_threads{state=\"active\"}",
          "legendFormat": "Active Threads",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Response Time (avg)",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_sum[1m]) / rate(http_response_time_histogram_count[1m])",
          "legendFormat": "Avg Response Time (ms)",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Transactions Per Second (TPS)",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_count[1m])",
          "legendFormat": "TPS",
          "refId": "E"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Success Count",
      "targets": [
        {
          "expr": "increase(http_success_total[1m])",
          "legendFormat": "Success Count",
          "refId": "C"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Failure Count",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "increase(http_failure_total[1m])",
          "legendFormat": "Failure Count",
          "refId": "D"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 24,
        "w": 12,
        "h": 8
      }
    }
  ]
}