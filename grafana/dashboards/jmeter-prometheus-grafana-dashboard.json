{
  "id": null,
  "uid": "jmeter-http-metrics",
  "title": "JMeter HTTP Metrics Dashboard",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 2,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m"],
    "time_options": ["5m", "10m", "15m", "30m"]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Active Threads",
      "targets": [
        {
          "expr": "jmeter_threads{state=\"active\"}",
          "legendFormat": "active threads",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Response Time (avg)",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_sum[1m]) / rate(http_response_time_histogram_count[1m])",
          "legendFormat": "avg response time (ms)",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Success Count",
      "targets": [
        {
          "expr": "http_response_time_histogram_count",
          "legendFormat": "success count",
          "refId": "C"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Transactions Per Second (TPS)",
      "targets": [
        {
          "expr": "rate(http_response_time_histogram_count[1m])",
          "legendFormat": "TPS",
          "refId": "D"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      }
    }
  ]
}
